{
    "version": "1",
    "enabled-engines": [
        "vertx",
        "postgres",
        "kafka",
        "flink"
    ],
    "script": {
        "main": "creditcard_analytics.sqrl"
    },
    "engines": {
        "flink": {
            "config": {
                "table.exec.source.idle-timeout": "10 s"
            }
        }
    },
    "connectors": {
        "filesystem": {
            "warehouse": "warehouse",
            "catalog-type": "hadoop",
            "catalog-name": "mydatabase"
        }
    },
    "dependencies": {
        "creditcard-data": {
            "folder": "creditcard-testdata"
        }
    }
}