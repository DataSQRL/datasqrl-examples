{
  "version": "1",
  "enabled-engines": ["vertx", "iceberg", "duckdb", "flink"],
  "script": {
    "main": "creditcard_views.sqrl",
    "config": {
      "variant": "testdata"
    }
  },
  "engines": {
    "flink": {
      "config" : {
        "table.exec.source.idle-timeout": "10 s"
      }
    }
  },
  "connectors" : {
    "iceberg" : {
      "warehouse":"warehouse",
      "catalog-type":"hadoop",
      "catalog-name": "mydatabase"
    }
  },
  "test-runner": {
    "delay-sec": 120
  }
}
