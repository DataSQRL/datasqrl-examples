{
  "version": "1",
  "enabled-engines": ["iceberg", "snowflake", "flink"],
  "script": {
    "main": "creditcard_views.sqrl"
  },
  "engines": {
    "flink": {
      "config" : {
        "table.exec.source.idle-timeout": "10 s"
      }
    },
    "snowflake" : {
      "catalog-name": "MyCatalog",
      "external-volume": "MyNewVolume",
      "url": "${SNOWFLAKE_JDBC_URL}"
    }
  },
  "connectors" : {
    "iceberg" : {
      "warehouse":"s3://my-iceberg-table-test",
      "catalog-impl":"org.apache.iceberg.aws.glue.GlueCatalog",
      "io-impl":"org.apache.iceberg.aws.s3.S3FileIO",
      "catalog-name": "mydatabase",
      "catalog-database": "mydatabase"
    }
  },
  "dependencies": {
    "creditcard-data": {
      "folder": "creditcard-kafka"
    }
  }
}
