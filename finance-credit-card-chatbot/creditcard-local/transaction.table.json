{
  "flink" : {
    "format" : "flexible-json",
    "path" : "${DATA_PATH}/transaction.jsonl",
    "source.monitor-interval" : "10 min",
    "connector" : "filesystem"
  },
  "version" : 1,
  "table" : {
    "type" : "source",
    "primary-key" : ["transactionId", "time"],
    "timestamp" : "time",
    "watermark-millis" : "1000"
  }
}