{
  "flink": {
    "format": "flexible-json",
    "path": "s3://${S3_DATA_BUCKET}/transaction.jsonl",
    "source.monitor-interval": 1,
    "connector": "filesystem"
  },
  "version": 1,
  "table": {
    "type": "source",
    "primary-key": [
      "transactionId",
      "time"
    ],
    "timestamp": "time",
    "watermark-millis": "1000"
  }
}