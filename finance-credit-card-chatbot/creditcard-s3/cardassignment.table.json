{
  "flink": {
    "format": "flexible-json",
    "path": "s3://${S3_DATA_BUCKET}/cardAssignment.jsonl",
    "source.monitor-interval": "1 min",
    "connector": "filesystem"
  },
  "version": 1,
  "table": {
    "type": "source",
    "primary-key": [
      "customerId",
      "cardNo",
      "timestamp"
    ],
    "timestamp": "timestamp",
    "watermark-millis": "1000"
  }
}