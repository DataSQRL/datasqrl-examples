{
  "version": "1",
  "enabled-engines": ["vertx", "iceberg", "duckdb", "flink"],
  "script": {
    "main": "creditcard_views.sqrl"
  },
  "engines": {
    "flink": {
      "connectors" : {
        "iceberg" : {
          "warehouse":"warehouse",
          "catalog-type":"hadoop",
          "catalog-name": "mydatabase"
        }
      },
      "config" : {
        "table.exec.source.idle-timeout": "10 s"
      }
    }
  },
  "test-runner": {
    "delay-sec": 120
  },
  "dependencies": {
    "creditcard-data": {
      "name": "creditcard-testdata",
      "version": "1",
      "variant": "dev"
    }
  }
}