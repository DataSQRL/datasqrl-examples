{
  "version": "1",
  "enabled-engines": ["vertx", "iceberg", "duckdb", "flink"],
  "script": {
    "main": "creditcard-views.sqrl"
  },
  "values" : {
    "flink-config": {
      "table.exec.source.idle-timeout": "10 sec"
    }
  },
  "engines": {
    "flink": {
      "connectors" : {
        "iceberg" : {
          "warehouse":"warehouse",
          "catalog-type":"hadoop",
          "catalog-name": "mydatabase"
        }
      }
    }
  },
  "test-runner": {
    "delay-sec": -1,
    "required-checkpoints": 1
  },
  "dependencies": [{
    "creditcard-data": {
      "name": "creditcard-local",
      "version": "1",
      "variant": "dev"
    }
  }]
}