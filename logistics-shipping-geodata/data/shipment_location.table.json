{
  "flink" : {
    "format" : "flexible-json",
    "path" : "${DATA_PATH}/shipment_location.jsonl",
    "source.monitor-interval" : 10000,
    "connector" : "filesystem"
  },
  "metadata" : {
    "_ingest_time" : {
      "attribute" : "proctime()"
    }
  },
  "version" : 1,
  "table" : {
    "type" : "source",
    "primary-key" : [ "timestamp", "shipmentId", "vehicleId" ],
    "timestamp" : "_ingest_time"
  }
}