{
  "flink" : {
    "format" : "flexible-json",
    "path" : "${DATA_PATH}/bolo.jsonl",
    "source.monitor-interval" : 10000,
    "connector" : "filesystem"
  },
  "version" : 1,
  "table" : {
    "type" : "source",
    "primary-key" : [ "bolo_id", "last_updated" ],
    "timestamp" : "last_updated",
    "watermark-millis" : 0
  }
}