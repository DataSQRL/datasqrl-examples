CREATE TABLE Addresses WITH (
    'connector' = 'filesystem',
    'format' = 'flexible-json',
    'path' = '${DATA_PATH}/addresses.jsonl'
) LIKE `addresses.jsonl`;

CREATE TABLE SinkTable WITH (
    'connector' = 'filesystem',
    'format' = 'flexible-json',
    'path' = '/tmp/sink/'
) LIKE `addresses.jsonl`;
