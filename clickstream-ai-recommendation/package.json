{
  "script": {
    "main": "recommendation.sqrl",
    "graphql": "recommendation.graphqls"
  },
  "profiles": [
    "datasqrl.profile.default",
    "profiles/redpanda"
  ],
  "values": {
    "mountDir": "${SQRL_DIR}/embedding:/embedding",
    "create-topics" : ["clickstream", "content"]
  },
  "dependencies": [{
    "clickstream" : {
      "name": "clickstream-kafka",
      "version": "1",
      "variant": "dev"
    },
    "content" : {
      "name": "content-kafka",
      "version": "1",
      "variant": "dev"
    }
  }]
}