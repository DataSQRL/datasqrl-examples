{
  "version": "1",
  "enabled-engines": ["flink", "iceberg", "kafka"],
  "script": {
    "main": "kafka-to-iceberg.sqrl"
  },
  "engines": {
    "flink": {
      "config": {
        "execution.checkpointing.interval": "1 min",
        "table.exec.source.idle-timeout": "60 s"
      }
    }
  },
  "connectors": {
    "iceberg": {
      "connector": "iceberg",
      "warehouse": "s3://my-iceberg-table-test",
      "catalog-impl": "org.apache.iceberg.aws.glue.GlueCatalog",
      "io-impl": "org.apache.iceberg.aws.s3.S3FileIO",
      "catalog-name": "mycatalog",
      "catalog-database": "mydatabase",
      "write.upsert.enabled": "true"
    }
  },
  "test-runner": {
    "create-topics": ["contacts"]
  }
}
