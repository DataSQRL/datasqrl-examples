{
  "version": "1",
  "enabled-engines": ["vertx", "postgres", "kafka", "flink"],
  "script": {
    "main": "kafka-test.sqrl"
  },
  "engines": {
    "flink": {
      "config": {
        "table.exec.source.idle-timeout": "10000 ms"
      }
    }
  },
  "test-runner": {
    "create-topics": ["contact", "organization", "enrichedcontact"]
  },
  "dependencies": {
    "kafkasource": {
      "folder": "kafka-source"
    },
    "kafkasink": {
      "folder": "kafka-sink"
    }
  }
}
