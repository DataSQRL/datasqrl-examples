{
  "flink" : {
    "format" : "flexible-json",
    "path" : "${DATA_PATH}/clinicalindicator.jsonl",
    "source.monitor-interval": "10 min",
    "connector" : "filesystem"
  },
  "metadata" : {
    "timestamp" : {
      "attribute" : "time.EpochMilliToTimestamp(time)"
    }
  },
  "version" : 1,
  "table" : {
    "type" : "source",
    "primary-key" : [ "sensorId", "time" ],
    "timestamp" : "timestamp",
    "watermark-millis" : 1
  }
}