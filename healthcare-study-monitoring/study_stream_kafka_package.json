{
  "version": "1",
  "enabled-engines": ["kafka", "flink"],
  "script": {
    "main": "study_stream.sqrl"
  },
  "values" : {
    "flink-config": {
      "table.exec.source.idle-timeout": "1000 ms"
    },
    "create-topics" : ["indicators", "enrichedindicators"]
  },
  "dependencies": [{
    "metrics" : {
      "name": "metrics-kafka",
      "version": "1",
      "variant": "dev"
    },
    "masterdata" : {
      "name": "masterdata-local",
      "version": "1",
      "variant": "dev"
    },
    "sink" : {
      "name": "sink-kafka",
      "version": "1",
      "variant": "dev"
    }
  }]
}