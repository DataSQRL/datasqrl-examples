{
  "version": "1",
  "enabled-engines": ["iceberg", "snowflake", "flink"],
  "script": {
    "main": "study_analytics.sqrl"
  },
  "engines": {
    "flink": {
      "config": {
        "table.exec.source.idle-timeout": "500 ms"
      }
    },
    "snowflake": {
      "catalog-name": "MyCatalog",
      "external-volume": "MyNewVolume",
      "url": "${SNOWFLAKE_JDBC_URL}"
    }
  },
  "connectors": {
    "iceberg": {
      "warehouse": "s3a://sqrl-examples-data-bucket/datasqrl-examples/healthcare-study-analytics-iceberg",
      "catalog-type": "hadoop",
      "catalog-name": "mycatalog"
    }
  }
}
